{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@sections/*": ["src/pages/_*"],
      "@ui/*": ["src/components/ui/*"],
      "@images/*": ["src/images/*"],
      "@services/*": ["src/services/*"],
    }
  }
}
